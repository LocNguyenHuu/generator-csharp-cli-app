{
  "name": "generator-csharp-cli-app",
  "version": "1.0.7",
  "description": "Yeoman generator for a C# command line app with log4net and unit tests.",
  "main": "app/index.js",
  "repository": "ngeor/generator-csharp-cli-app",
  "scripts": {
    "test": "gulp",
    "coverage": "nyc mocha",
    "coveralls": "nyc mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "Nikolaos Georgiou",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.5.7",
    "uuid": "^3.1.0",
    "yeoman-generator": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-ngeor": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.1",
    "gulp-mocha": "^5.0.0",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "app/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "all": true
  }
}
